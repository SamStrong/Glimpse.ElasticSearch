<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Glimpse.ElasticSearch</id>
        <version>1.0</version>
        <title>A simple Glimpse plugin for ElasticSearch</title>
        <authors>Onuralp Taner | @onuralptaner</authors>
        <projectUrl>https://github.com/onuralp/Glimpse.ElasticSearch</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Adds a ElasticSearch tab to your Glimpse console that shows queries and request time information.
Server queries are included in the Glimpse timeline.</description>
        <tags>elasticsearch nest glimpse profiling</tags>
        <dependencies>
            <dependency id="Elasticsearch.Net" version="1.2.3" />
            <dependency id="Glimpse.AspNet" version="1.9.2" />
        </dependencies>
    </metadata>
    <files>
        <file src="lib\net40\Glimpse.ElasticSearch.dll" target="lib\net40\Glimpse.ElasticSearch.dll" />
        <file src="lib\net40\Glimpse.ElasticSearch.pdb" target="lib\net40\Glimpse.ElasticSearch.pdb" />
    </files>
</package>